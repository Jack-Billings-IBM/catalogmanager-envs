{
   "$schema":"http://json-schema.org/draft-04/schema#",
   "description":"Response schema for the DFH0XCMN JSON interface",
   "type":"object",
   "properties":{
      "DFH0XCMNOperationResponse":{
         "type":"object",
         "properties":{
            "ca_request_id":{
               "type":"string",
               "maxLength":6
            },
            "ca_return_code":{
               "type":"integer",
               "maximum":99,
               "minimum":0
            },
            "ca_response_message":{
               "type":"string",
               "maxLength":79
            },
            "ca_inquire_request":{
               "type":"object",
               "properties":{
                  "ca_list_start_ref":{
                     "type":"integer",
                     "maximum":9999,
                     "minimum":0
                  },
                  "ca_last_item_ref":{
                     "type":"integer",
                     "maximum":9999,
                     "minimum":0
                  },
                  "ca_item_count":{
                     "type":"integer",
                     "maximum":999,
                     "minimum":0
                  },
                  "ca_cat_item":{
                     "type":"array",
                     "maxItems":15,
                     "minItems":15,
                     "items":{
                        "type":"object",
                        "properties":{
                           "ca_item_ref":{
                              "type":"integer",
                              "maximum":9999,
                              "minimum":0
                           },
                           "ca_description":{
                              "type":"string",
                              "maxLength":40
                           },
                           "ca_department":{
                              "type":"integer",
                              "maximum":999,
                              "minimum":0
                           },
                           "ca_cost":{
                              "type":"string",
                              "maxLength":6
                           },
                           "in_stock":{
                              "type":"integer",
                              "maximum":9999,
                              "minimum":0
                           },
                           "on_order":{
                              "type":"integer",
                              "maximum":999,
                              "minimum":0
                           }
                        },
                        "required":[
                           "ca_item_ref",
                           "ca_description",
                           "ca_department",
                           "ca_cost",
                           "in_stock",
                           "on_order"
                        ]
                     }
                  }
               },
               "required":[
                  "ca_list_start_ref",
                  "ca_last_item_ref",
                  "ca_item_count",
                  "ca_cat_item"
               ]
            }
         },
         "required":[
            "ca_request_id",
            "ca_return_code",
            "ca_response_message",
            "ca_inquire_request"
         ]
      }
   },
   "required":[
      "DFH0XCMNOperationResponse"
   ]
}